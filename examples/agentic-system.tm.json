  "system": {
    "name": "Research Assistant Agent",
    "type": "agentic-system",
    "threat_modeling_framework": "owasp-agentic-top10-2026",
    "components": [
      {
        "id": "research-agent",
        "name": "Research Agent",
        "type": "agent",
        "capabilities": [
          "web-search",
          "document-analysis",
          "report-generation"
        ],
        "trust_level": "internal",
        "description": "Main research agent"
      },
      {
        "id": "llm-backend",
        "name": "LLM Backend",
        "type": "llm",
        "capabilities": [
          "reasoning",
          "text-generation"
        ],
        "trust_level": "internal",
        "description": "LLM backend for agent reasoning"
      },
      {
        "id": "web-search-tool",
        "name": "Web Search Tool",
        "type": "tool",
        "capabilities": [
          "web-search"
        ],
        "trust_level": "untrusted",
        "description": "External web search tool"
      },
      {
        "id": "file-system-tool",
        "name": "File System Tool",
        "type": "tool",
        "capabilities": [
          "read-file",
          "write-file"
        ],
        "trust_level": "internal",
        "description": "File system access tool"
      },
      {
        "id": "agent-memory",
        "name": "Agent Memory",
        "type": "memory",
        "capabilities": [
          "store-context",
          "retrieve-context"
        ],
        "trust_level": "internal",
        "description": "Vector database for agent memory"
      }
    ],
    "data_flows": [
      {
        "from": "research-agent",
        "to": "llm-backend",
        "data_type": "reasoning-request",
        "classification": "internal",
        "protocol": "HTTP",
        "encrypted": false
      },
      {
        "from": "llm-backend",
        "to": "research-agent",
        "data_type": "reasoning-response",
        "classification": "internal",
        "protocol": "HTTP",
        "encrypted": false
      },
      {
        "from": "research-agent",
        "to": "web-search-tool",
        "data_type": "search-query",
        "classification": "internal",
        "protocol": "HTTPS",
        "encrypted": true
      },
      {
        "from": "research-agent",
        "to": "file-system-tool",
        "data_type": "file-operation",
        "classification": "confidential",
        "protocol": "local",
        "encrypted": false
      },
      {
        "from": "research-agent",
        "to": "agent-memory",
        "data_type": "context-data",
        "classification": "internal",
        "protocol": "TCP",
        "encrypted": false
      }
    ]
  },
