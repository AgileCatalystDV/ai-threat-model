      "id": "af1a9f69-d87c-4ed3-848c-fff15264ba0d",
      "category": "AGENTIC01",
      "framework": "owasp-agentic-top10-2026",
      "title": "Agent Manipulation",
      "description": "Agent manipulation occurs when attackers influence agent behavior through malicious inputs or environment manipulation. This can lead to unauthorized actions or data exposure.",
      "severity": "critical",
      "affected_components": [
        "research-agent"
      ],
      "affected_data_flows": [],
      "attack_vectors": [
        "Prompt injection to manipulate agent behavior",
        "Environment manipulation",
        "State corruption attacks",
        "Goal hijacking"
      ],
      "detection_patterns": [
        "Agent receives untrusted input without validation",
        "No input sanitization for agent prompts",
        "Agent state can be manipulated externally",
        "No validation of agent decisions"
      ],
      "mitigations": [
        {
          "id": "input-validation",
          "description": "Validate and sanitize all agent inputs",
          "implementation": "Implement input validation and sanitization for all agent inputs.",
          "status": "proposed",
          "priority": "high"
        },
        {
          "id": "state-protection",
          "description": "Protect agent state from unauthorized modification",
          "implementation": "Implement state validation and integrity checks. Use immutable state where possible.",
          "status": "proposed",
          "priority": "high"
        },
        {
          "id": "decision-validation",
          "description": "Validate agent decisions before execution",
          "implementation": "Implement decision validation and approval workflows for critical actions.",
          "status": "proposed",
          "priority": "medium"
        }
      ],
      "risk_score": null,
      "references": []
    },
